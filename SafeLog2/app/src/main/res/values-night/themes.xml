<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Base application theme. -->
    <style name="Theme.SafeLog" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/TitleBarColour</item>

        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/PrimaryBackgroundDark</item>
        <item name="editTextBackground">@drawable/edittext_dark</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:navigationBarColor">@color/TitleBarColour</item>
        <item name="search_icon">@drawable/ic_search_icon_dark</item>
        <item name="card_background">@color/CardBackgroundDark</item>
        <item name="edit_icon">@drawable/ic_edit_icon_dark</item>
        <item name="group_head_color">@color/white</item>
       <item name="android:scrollbarThumbVertical">@color/TitleBarColour</item>
        <item name="dialog_background">@color/PrimaryBackgroundDark</item>
        <item name="android:editTextBackground">@drawable/edittext_dark</item>
        <item name="blockspiner_background">@drawable/blockspinnerbackground_dark</item>
        <item name="android:popupBackground">@color/PrimaryBackgroundDark</item>
        <item name="copy_icon">@drawable/ic_copyicon_dark</item>
        <item name="eye_icon">@drawable/ic_eyeicon_dark</item>
        <item name="group_col">@color/TextboxColourDark</item>
        <item name="ic_lock">@drawable/ic_lock_dark</item>


    </style>
</resources>